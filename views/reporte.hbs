<html><head>
    <meta charset="utf-8">
    <title>Controles HLS</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script type="text/javascript" src="http://cdnjs.cloudflare.com/ajax/libs/jquery/2.0.3/jquery.min.js"></script>
    <script type="text/javascript" src="http://netdna.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js"></script>
    <link href="http://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="http://pingendo.github.io/pingendo-bootstrap/themes/default/bootstrap.css" rel="stylesheet" type="text/css">
    <script type="text/javascript" src="js/jquery.js"></script>
    <script type="text/javascript" src="js/bootstrap.js"></script>
    <link href="css/font-awesome.css" rel="stylesheet" type="text/css">
    <link href="css/bootstrap.css" rel="stylesheet" type="text/css">
    <link href="css/bootstrap-datepicker.css" rel="stylesheet" type="text/css">
    <script type="text/javascript" src="js/bootstrap-datepicker.js"></script>
    <script type="text/javascript" src="locales/bootstrap-datepicker.es.min.js"></script>
  </head><body>
    <div class="navbar navbar-default navbar-static-top">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-ex-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="/"><span>CONTROLES HLS</span></a>
        </div>
        <div class="collapse navbar-collapse" id="navbar-ex-collapse">
          <ul class="nav navbar-nav navbar-right">
            <li class="hidden-sm hidden-xs">
              <a href="/"><img src="home.png" alt="Inicio" width="25" height="25"></a>
            </li>
            <li>
              <a href="/">Inicio</a>
            </li>
          </ul>
        </div>
      </div>
    </div>
    <div class="section section-primary text-center">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <ul class="media-list">
              <li class="media">
                <a class="pull-left" href="/"><img class="img-circle media-object" src="logo1.png" height="64" width="64"></a>
                <div class="media-body">
                  <h1 class="text-center">{{titulo}}</h1>
                </div>
              </li>
              <li class="media"></li>
            </ul>
          </div>
        </div>
      </div>
    </div>
    <div class="section">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <form class="form-horizontal" role="form" action="reporteCreado" method="post" >
            <div class="form-group">
                <div class="col-sm-2">
                  <label for="inputEmail3" class="control-label">Cliente</label>
                </div>
                <div class="col-sm-10">
                  <div class="col-md-4">
                    <select class="form-control input-lg" id="sel1" name="cliente">
                       <option disabled selected value> Seleccione Cliente </option>
                        {{#each clientes}}
                            <option>{{cliente}}</option>
                        {{/each}}
                    </select>
                  </div>
                </div>
              </div>
              <div class="form-group">
                <div class="col-sm-2">
                  <label for="inputEmail3" class="control-label">Asesor</label>
                </div>
                <div class="col-sm-10">
                  <div class="col-md-4">
                    <select class="form-control input-lg" id="sel1" name="asesor">
                       <option selected> {{asesor}}</option>
                    </select>
                  </div>
                </div>
              </div>
              <div class="form-group">
                <div class="col-sm-2">
                  <label for="inputPassword3" class="control-label">Clave</label>
                </div>
                <div class="col-sm-10">
                  <input type="password" class="form-control" id="inputPassword3" placeholder="Password" name="clave">
                </div>
              </div>
              <div class="form-group">
                <div class="col-sm-2">
                  <label class="control-label">Fecha</label>
                </div>
                <div class="col-sm-10">
                  <div id="sandbox-container">
                    <div class="input-group date">
                      <input type="text" class="form-control" name="fecha" value="16/03/2016">
                      <div class="input-group-addon">
                        <span class="fa fa-calendar fa-fw fa-lg"></span>
                      </div>
                    </div>
                  </div>
                </div>
              </div>

              <div class="form-group">
                <div class="col-sm-2">
                  <label for="inputEmail3" class="control-label">H. Inicio</label>
                </div>
                <div class="col-sm-10">
                  <div class="col-md-4">
                    <select class="form-control input-lg" id="sel1" name="hini">
                      {{#each horas}}
                            <option>{{hora}}</option>
                        {{/each}}
                    </select>
                  </div>
                  <div class="col-md-4">
                    <select class="form-control input-lg" id="sel1" name="mini">
                      {{#each minutos}}
                            <option>{{minuto}}</option>
                        {{/each}}
                    </select>
                  </div>
                </div>
              </div>

              <div class="form-group">
                <div class="col-sm-2">
                  <label for="inputEmail3" class="control-label">H. Fin</label>
                </div>
                <div class="col-sm-10">
                  <div class="col-md-4">
                    <select class="form-control input-lg" id="sel1" name="hfin">
                      {{#each horas}}
                            <option>{{hora}}</option>
                        {{/each}}
                    </select>
                  </div>
                  <div class="col-md-4">
                    <select class="form-control input-lg" id="sel1" name="mfin">
                      {{#each minutos}}
                            <option>{{minuto}}</option>
                        {{/each}}
                    </select>
                  </div>
                </div>
              </div>


              <div class="form-group">
                <div class="col-sm-2">
                  <label for="inputEmail3" class="control-label">H. Facturadas</label>
                </div>
                <div class="col-sm-10">
                  <div class="col-md-4">
                    <select class="form-control input-lg" id="sel1" name="hfact">
                      {{#each horas}}
                            <option>{{hora}}</option>
                        {{/each}}
                    </select>
                  </div>
                  <div class="col-md-4">
                    <select class="form-control input-lg" id="sel1" name="mfact">
                      {{#each minutos}}
                            <option>{{minuto}}</option>
                        {{/each}}
                    </select>
                  </div>
                </div>
              </div>

              <div class="form-group">
                <div class="col-sm-2">
                  <label for="inputPassword3" class="control-label">Observaciones</label>
                </div>
                <div class="col-sm-10">
                  <input type="text" class="form-control" id="inputPassword3" placeholder="Observaciones" name="obs">
                </div>
              </div>
              <div class="form-group">
                <div class="col-sm-offset-2 col-sm-10">
                  <button type="submit" class="btn btn-block btn-lg btn-primary" name="Enviar" value="funcion">CREAR</button>
                </div>
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>
    
    <script type="text/javascript">
$('#sandbox-container .input-group.date').datepicker({
    format: "dd/mm/yyyy",
    language: "es",
    autoclose: true,
    toggleActive: true
});
    </script>



</body></html>