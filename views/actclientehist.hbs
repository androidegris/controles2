<html><head>
        <meta charset="utf-8">
        <title>Controles HLS</title>
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <script type="text/javascript" src="http://cdnjs.cloudflare.com/ajax/libs/jquery/2.0.3/jquery.min.js"></script>
        <script type="text/javascript" src="http://netdna.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js"></script>
        <link href="http://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">
        <link href="http://pingendo.github.io/pingendo-bootstrap/themes/default/bootstrap.css" rel="stylesheet" type="text/css">
        <script type="text/javascript" src="js/jquery.js"></script>
        <script type="text/javascript" src="js/bootstrap.js"></script>
        <link href="css/font-awesome.css" rel="stylesheet" type="text/css">
        <link href="css/bootstrap.css" rel="stylesheet" type="text/css">
    </head><body>
        <div class="navbar navbar-default navbar-static-top">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-ex-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="/"><span>CONTROLES HLS</span></a>
                </div>
                <div class="collapse navbar-collapse" id="navbar-ex-collapse">
                    <ul class="nav navbar-nav navbar-right">
                        <li class="hidden-sm hidden-xs">
                            <a href="/"><img src="home.png" alt="Inicio" width="25" height="25"></a>
                        </li>
                        <li>
                            <a href="/">Inicio</a>
                        </li>
                        <li>
              <a href="/historico">Historico</a>
            </li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="section section-success text-center">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <ul class="media-list">
                            <li class="media">
                                <a class="pull-left" href="historico"><img class="img-circle media-object" src="logo1.png" height="64" width="64"></a>
                                <div class="media-body">
                                    <h1 class="text-center">{{titulo}}</h1>
                                </div>
                            </li>
                            <li class="media"></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="section">
            <div class="container">
                <div class="row">
                    <div class="col-md-10">
                        <h3>Asesor: {{asesor}} &nbsp; &nbsp; &nbsp; &nbsp; Cliente: &nbsp;{{cliente}}
                            &nbsp; &nbsp; &nbsp;Fecha: &nbsp;{{fecha}}</h3>
                    </div>
                    <div class="col-md-2 text-right">
                        <button onclick="goBack()" class="btn btn-block btn-lg btn-info">Regresar</button>
            
                    </div>
                </div>
            </div>
        </div>
        <h3 class="text-center">Lista Actividades</h3>
        <div class="section">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <ul class="list-group">{{#each reportes}}
                            <li class="list-group-item">-- {{obs}}</li>{{/each}}</ul>
                    </div>
                </div>
            </div>
        </div>
    
<script>
function goBack() {
    window.history.back();
}
</script>
  
</body></html>