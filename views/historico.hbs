                  <form role="form" action="historicoSubirReportesCliente" method="post">
                    <div class="form-group has-success">
                      <select class="form-control input-lg" id="sel1" name="cliente">
                        <option disabled selected value> Seleccione Cliente </option>
                        {{#each clientes}}
                        <option>{{cliente}}</option>
                        {{/each}}
                      </select>
                    </div>
                    <button type="submit" class="btn btn-block btn-lg btn-success" name="Enviar" value="Sign up">VER</button>
                  </form>
